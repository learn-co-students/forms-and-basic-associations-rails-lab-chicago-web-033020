<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />

  <%=f.label :artist_name %>
  <%= f.text_field :artist_name %>

  <%=f.label :song_genre_id%>
  <%=f.collection_select :genre_id, Genre.all, :id, :name %>
 
  <%=f.fields_for :notes do |note_form| %>
    <%= note_form.label :note %>
    <%= note_form.text_area :content %>
    <%end%>
  

  <%= f.submit %>
<% end %>


